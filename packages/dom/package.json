{
  "name": "@dnd-kit/dom",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@dnd-kit/abstract": "*",
    "@dnd-kit/collision": "*",
    "@dnd-kit/dom-utilities": "*",
    "@dnd-kit/geometry": "*",
    "@dnd-kit/state": "*",
    "@dnd-kit/utilities": "*"
  },
  "devDependencies": {
    "@dnd-kit/config-eslint": "*",
    "@dnd-kit/config-ts": "*",
    "eslint": "^8.38.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
